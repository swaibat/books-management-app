{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "nyc": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "exclude": [
      "**/**/**/index.js"
    ]
  },
  "scripts": {
    "start": "babel-node index.js",
    "dev": "nodemon --exec babel-node index.js",
    "test": "npm run db-migrate:test && npm run db-seeder:test && nyc mocha  --timeout 200000 --exit",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "db-migrate": "sequelize db:migrate --env production",
    "db-migrate:dev": "sequelize db:migrate --env development",
    "db-seeder:dev": "sequelize db:seed:all --env development",
    "db-seeder:test": "sequelize db:seed:all --env test",
    "db-delete:dev": "sequelize db:migrate:undo:all --env development",
    "db-migrate:test": "sequelize db:migrate:undo:all --env development && sequelize db:migrate --env development"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.10.0",
    "@babel/register": "^7.9.0",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "jsonwebtoken": "^8.5.1",
    "node": "^14.3.0",
    "nodemailer": "^6.4.6",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.11",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "babel-plugin-istanbul": "^5.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1"
  }
}
