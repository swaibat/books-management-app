    "@babel/register": "^7.9.0",
    "bcrypt": "^4.0.1",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "jsonwebtoken": "^8.5.1",
    "node": "^14.3.0",
    "nodemailer": "^6.4.6",
    "nyc": "^15.0.1",
